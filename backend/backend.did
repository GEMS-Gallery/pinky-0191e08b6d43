type Result_1 = 
 variant {
   err: text;
   ok: float64;
 };
type Result = 
 variant {
   err: text;
   ok: text;
 };
type CalculationResult = 
 record {
   operation: text;
   result: float64;
 };
service : {
  calculate: (text, float64, float64) -> (Result_1);
  getHistory: () -> (vec CalculationResult) query;
  sendEmail: (text, text, text) -> (Result);
}
